@{
    ViewData["Title"] = "Fixed";
}


<form asp-action="Fixed">
    <div class="form-group">
        <label class="control-label">Số lần sửa</label>
        <input name="solansua" class="form-control" />
    </div>
    <div class="form-group">
        <input type="submit" value="Liệt kê" class="btn btn-primary" />
    </div>
</form>

@if (ViewData["list"] != null)
{
<table border="1" cellspacing="0" cellpadding="10px">
    <tr>
        <td>Tên Nhân Viên</td>
        <td>Số điện thoại</td>
        <td>Số lần sửa</td>
    </tr>
    @foreach (var list in ViewData["list"] as IList<Fixed>)
    {
        <tr>
            <td>@list.TenNhanVien</td>
            <td>@list.SoDT</td>
            <td>@list.LanThu</td>
        </tr>
    }

</table>

}

