@model TH05.Models.NhanVien

@{
    ViewData["Title"] = "Index";
}
<form asp-action="LietKe" asp-controller="NV_BT" method="post">
    <div class="form-group">
        <label class="control-label">Tên Nhân Viên</label>
        <select asp-for="MaNhanVien" class="form-control">
            @foreach (NhanVien nv in (List<NhanVien>)ViewBag.dsNV)
            {
                <option value="@nv.MaNhanVien">@nv.TenNhanVien</option>
            }
        </select>
    </div>
    <div class="form-group">
        <input type="submit" value="Liệt kê" class="btn btn-primary" />
    </div>
</form>

